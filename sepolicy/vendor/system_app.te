# Allow system_app to read batteryhealth node
allow system_app sysfs_batteryinfo:dir r_dir_perms;
allow system_app sysfs_batteryinfo:file r_file_perms;

# Allow system_app to read fps info node
allow system_app sysfs_fps:dir search;
allow system_app sysfs_fps:file r_file_perms;

# Allow platform_app to read fps info node
allow platform_app sysfs_fps:dir search;
allow platform_app sysfs_fps:file r_file_perms;
